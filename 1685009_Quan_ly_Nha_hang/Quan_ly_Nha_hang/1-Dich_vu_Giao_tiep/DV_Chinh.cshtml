@{
    var Ung_dung = XL_UNG_DUNG.Khoi_dong_Ung_dung();

    var Ma_so_Xu_ly = "";
    var Chuoi_Kq = "";

    if (Request["Ma_so_Xu_ly"] != null)
    {
        var Bo_doc = new StreamReader(Request.InputStream);
        var Chuoi_Nhan = Bo_doc.ReadToEnd();
        Ma_so_Xu_ly = Request["Ma_so_Xu_ly"];

        if (Ma_so_Xu_ly == "GHI_PHIEU_DAT_BAN_MOI")
        {
            var Phieu_Dat_Ban = Json.Decode<XL_PHIEU_DAT_BAN>(Chuoi_Nhan);
            Chuoi_Kq = Ung_dung.Ghi_Phieu_Dat_Ban_Moi(Phieu_Dat_Ban);
        }
        else if (Ma_so_Xu_ly == "GHI_TRANG_THAI_DA_NAU")
        {
            var Ma_so_Mon_an = Request["Ma_so_Mon_an"];
            var Ma_so_Goi_mon = Request["Ma_so_Goi_mon"];
            
            Chuoi_Kq = Ung_dung.Ghi_Sua_Trang_Thai_Goi_Mon(Ma_so_Mon_an, Ma_so_Goi_mon, "DA_NAU");
        }
    }

    Response.ContentType = "text/json";
    Response.Write(Chuoi_Kq);
}